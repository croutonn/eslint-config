{
  "name": "@typehut/eslint-config",
  "version": "1.2.1",
  "description": "This is eslint-config for us.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typehut/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/typehut/eslint-config/issues"
  },
  "homepage": "https://github.com/typehut/eslint-config#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "@typehut",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "styleguide"
  ],
  "files": [
    "rulesets",
    "LICENSE",
    "package.json",
    "index.js",
    "esmodule.js",
    "javascript.js",
    "javascript-loose.js",
    "javascript-react.js",
    "javascript-react-loose.js",
    "javascript-test.js",
    "typescript-declare.js",
    "typescript.js",
    "typescript-loose.js",
    "typescript-react.js",
    "typescript-react-loose.js",
    "typescript-test.js"
  ],
  "main": "index.js",
  "scripts": {
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint . --ext js --fix",
    "lint": "eslint .",
    "pretests-only": "node ./test/requires",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "yarn run --silent lint",
    "test": "yarn run --silent tests-only",
    "version": "yarn fix"
  },
  "dependencies": {
    "@typehut/eslint-plugin": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "babel-tape-runner": "^3.0.0",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "tape": "^5.2.2"
  }
}
