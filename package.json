{
  "name": "@croutonn/eslint-config",
  "version": "1.0.0",
  "description": "This is eslint-config for me.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croutonn/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/croutonn/eslint-config/issues"
  },
  "homepage": "https://github.com/croutonn/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "author": "@croutonn",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "styleguide"
  ],
  "main": "index.js",
  "scripts": {
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint . --ext js --fix",
    "lint": "eslint .",
    "pretests-only": "node ./test/requires",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "yarn run --silent lint",
    "test": "yarn run --silent tests-only",
    "version": "fix"
  },
  "dependencies": {
    "@croutonn/eslint-plugin": "^0.1.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "babel-tape-runner": "^3.0.0",
    "eslint": "^7.11.0",
    "prettier": "^2.1.2",
    "react": "^17.0.0",
    "tape": "^5.0.1"
  }
}
